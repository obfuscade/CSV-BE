{
  "env": {
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import", "node", "prettier"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "func-names": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off"
  }
}
